        <div class="row">
          <h3>게시글</h3>
          <div></div>
          <% mypage_post.forEach((data) => { %>
            <div class="col-lg-4 col-md-6" data-post ="mypage-post-div" >
              <div class="icon-box" data-aos="fade-up">
                <div class="icon"><i class="bi bi-briefcase"></i></div>
                <h4 class="title"><a href=""><%= data.title %></a></h4>
                <p class="post-id" hidden> <%= data.post_id %> </p>
                <p class="post-description"> <%= data.content %> </p>
                <div class = "post-edit-open" hidden>
                  <div><textarea class = "post-edit-input"></textarea></div>
                  <div class = "post-edit-contents" >
                    <button id = "post-insert" >입력</button>
                    <button id = "post-cancel" >취소</button>
                  </div>
                </div>
                <div class = "post-edit-buttons">
