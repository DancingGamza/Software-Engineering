                <p class="comment-id" hidden> <%= data.comment_id %> </p>
                <p class="comment-description"><%= data.content %></p>
                <div class = "comment-edit-open" hidden>
                  <div><textarea class = "comment-edit-input"></textarea></div>
                  <div class = "comment-edit-contents" >
                    <button id = "comment-insert" >입력</button>
                    <button id = "comment-cancel" >취소</button>
                  </div>
                </div>
                <div class = "comment-edit-buttons">
                  <button id = "comment-delete">삭제</button>
                  <button id = "comment-edit">수정</button>
                </div>
              </div>
